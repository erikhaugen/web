<?xml version="1.0"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
			 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
			 version="2.0">
	
<!-- 	<persistence-unit name="evernote" transaction-type="JTA"> -->
	<persistence-unit name="registration" transaction-type="RESOURCE_LOCAL">
		<properties>
			<property name="javax.persistence.jdbc.driver"		value="${vectordb.jdbc.driverClassName}" />
			<property name="javax.persistence.jdbc.user"		value="${vectordb.jdbc.username}" />
			<property name="javax.persistence.jdbc.password"	value="${vectordb.jdbc.password}" />
			<property name="javax.persistence.jdbc.url"			value="${vectordb.jdbc.url}" />
<!-- 			<property name="hibernate.transaction.manager_lookup_class"  value="com.atomikos.icatch.jta.hibernate3.TransactionManagerLookup"/> -->
		</properties>
	</persistence-unit>
	
</persistence>
