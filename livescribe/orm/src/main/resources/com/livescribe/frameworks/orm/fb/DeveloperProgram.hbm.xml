<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.livescribe.frameworks.orm.fb">
	<class name="DeveloperProgram" table="DeveloperProgram">
	
		<id name="primaryKey" column="primaryKey" type="binary">
			<generator class="assigned"/>
		</id>
		
		<property name="displayName"	column="displayName"	type="string"	length="128" />
		<property name="isDefault"		column="isDefault"		type="integer" />
		<property name="name"			column="name"			type="string"	length="64" />
		<property name="sortOrder"		column="sortOrder"		type="integer" />
		
	</class>
</hibernate-mapping>
