<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.livescribe.frameworks.orm.fb.DupeEmailDeprecated" table="dupeEmailDeprecated">
	
		<id name="primaryKey" column="primaryKey" type="binary">
			<generator class="assigned"/>
		</id>
		
		<property name="deprecatedAccount"	column="deprecatedAccount"	type="integer" />
		<property name="shortId"			column="shortId"			type="string"	length="64" />
		<property name="email"				column="email"				type="string"	length="128" />
		
	</class>
</hibernate-mapping>
		